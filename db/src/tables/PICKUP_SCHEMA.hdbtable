COLUMN TABLE "PICKUP_SCHEMA" (
  "NAME" NVARCHAR (255) NOT NULL COMMENT 'Name',
  "TYPE" NVARCHAR (50) COMMENT 'Type',
  "INSTANCE_NAME" NVARCHAR (255) COMMENT 'Instance Name',
  "RESOURCE_NAME" NVARCHAR (255) COMMENT 'Resource Name',
  "PLAN" NVARCHAR (255) COMMENT 'Service Plan',
  "SCHEMA" NVARCHAR (255) COMMENT 'Schema',
  PRIMARY KEY(NAME)
) COMMENT 'Pickup Schemas'